# Exemplo de .env para o backend
# Copie este arquivo para ".env" e ajuste os valores antes de rodar.
# NÃO commite o arquivo .env com credenciais reais no repositório.

DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=
DB_NAME=clinica

# Porta onde a API irá escutar
PORT=3001

# Caso queira limitar CORS, defina ORIGIN (ex: http://localhost:5173)
CORS_ORIGIN=*
